<?xml version="1.0"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/4.7/phpunit.xsd"
	bootstrap="./vendor/autoload.php"
	beStrictAboutTestsThatDoNotTestAnything="true" beStrictAboutCoversAnnotation="true"
 	beStrictAboutOutputDuringTests="true" beStrictAboutChangesToGlobalState="true"
	verbose="true">
	<testsuites>
		<testsuite name="url">
			<directory>tests</directory>
		</testsuite>
	</testsuites>
	<php>
		<ini name="error_reporting" value="-1" />
	</php>
</phpunit>
